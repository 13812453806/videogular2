{
  "name": "videogular2",
  "version": "2.0.0-beta.0",
  "description": "Videogular is a video application framework for desktop and mobile powered by Angular 2.0",
  "main": "videogular2.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/videogular/videogular2"
  },
  "dependencies": {
    "angular2": "2.0.0-beta.1",
    "rxjs": "5.0.0-beta.0"
  },
  "devDependencies": {
    "http-server": "0.8.5",
    "jasmine-core": "2.4.1",
    "karma": "0.13.19",
    "karma-chrome-launcher": "0.2.2",
    "karma-coverage": "0.5.3",
    "karma-jasmine": "0.3.6",
    "phantomjs": "1.9.19",
    "remap-istanbul": "0.5.1",
    "systemjs": "0.19.16",
    "typescript": "^1.7.5"
  },
  "scripts": {
    "watch": "tsc -p src -w",
    "build": "rm -rf dist && tsc -p src",
    "test": "karma start karma.conf.js && node_modules/.bin/remap-istanbul -i coverage/coverage-final.json -o coverage -t html",
    "coverage": "http-server -c-1 -o -p 9875 ./coverage"
  },
  "engines": {
    "node": ">=0.8.0"
  },
  "keywords": [
    "video",
    "player",
    "framework",
    "angular2",
    "javascript"
  ],
  "author": "Raúl Jiménez <elecash@gmail.com> (http://twofuckingdevelopers.com/)",
  "contributors": [
    {
      "name": "Alberto García",
      "email": "cernhelm@gmail.com"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/videogular/videogular2/issues"
  },
  "homepage": "http://videogular.com"
}
